pipeline{
    agent any
    stages{
        stage("check code"){
            steps{
                git url:"https://github.com/mohan653/java-sample-app.git";branch:"mohan"
            }
        }
        stage("build"){
            steps{
                sh "mvn clean install"
            }
        }
        stage("test"){
            steps{
                sh "mvn test"
            }
        }
    }
}